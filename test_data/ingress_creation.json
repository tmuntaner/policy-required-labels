{
  "uid": "1299d386-525b-4032-98ae-1949f69f9cfc",
  "kind": {
    "group": "extensions",
    "kind": "Ingress",
    "version": "v1beta1"
  },
  "object": {
    "metadata": {
      "name": "prod"
    },
    "spec": {
      "tls": [
        {
          "hosts": ["some-host.com"],
          "secretName": "secret-name-tls"
        }
      ],
      "rules": [
        {
          "host": "some-host.com",
          "http": {
            "paths": [
              {
                "path": "/",
                "backend": {
                  "service": {
                    "name": "service",
                    "port": 443
                  }
                }
              }
            ]
          }
        }
      ]
    }
  }
}
